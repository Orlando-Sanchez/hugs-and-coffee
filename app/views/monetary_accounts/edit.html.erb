<div class="profile-edit-view" >
  <div id="profile-menu">
    <div class="profile-menu-header">
      <%= link_to @profile do %>
        <div class="header-logo second-bkgrnd">
          <h1 class="logo-font second-font ">A&C</h1> 
        </div>      
      <% end %>
      <%= image_tag("Vectorprofilemenu.svg", id: "vectorprofilemenu-img") %>
    </div>
    <%= render 'layouts/admin_menu' %>
  </div>
  <div id="profile-admin-dark">
    <div id="profile-form-main">
      <%= render 'layouts/admin_header' %>
      <div class="profile-form-desktop primary-bkgrnd">
        <%= render 'layouts/admin-menu-desktop' %>
        <div class="account-form-container">
          <div class="publication-form-header">
            <div class="profile-form-title primary-font">Editar cuenta</div>
          </div>
          <div class="form-wrapper">
            <%= render 'form_edit', object: @monetary_account %>       
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
 